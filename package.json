{
  "name": "play-pro",
  "version": "1.0.0",
  "description": "Playwright Demo",
  "main": "index.js",
  "scripts": {
    "test": "jest --detectOpenHandles",
    "code": "playwright codegen",
    "help": "playwright --help"

  },
  "keywords": [],
  "author": "Carlos Pardo Velazquez",
  "license": "ISC",
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)"
    ],
    "reporters": [
      "default",
      [
        "jest-html-reporters",
        {
          "publicPath": "./html-report",
          "filename": "report.html",
          "expand": true,
          "openReport": true
        }
      ]
    ]
  },
  "devDependencies": {
    "chalk": "^4.1.1",
    "jasmine-data-provider": "^2.2.0",
    "jest": "^26.6.3",
    "jest-html-reporters": "^2.1.6",
    "jest-playwright-preset": "^1.5.2",
    "playwright": "^1.11.0"
  }
}
